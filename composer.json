{
    "name": "cloudycity/tencent-marketing-sdk",
    "description": "Tencent MarketingAPI SDK",
    "keywords": [
        "tencent",
        "marketing",
        "sdk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "cloudycity",
            "email": "cloudycity@foxmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^6.2",
        "nesbot/carbon": "~1.0",
        "doctrine/collections": "~1.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "CloudyCity\\TencentMarketingSDK\\": "src/"
        },
        "files": [
            "src/Kernel/Support/Helpers.php"
        ]
    },
    "scripts": {
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    }
}
